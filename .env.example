# API Configuration
# Copy this file to .env and fill in your API keys

# Routing API (OpenRouteService or Mapbox)
ROUTING_API_KEY=your_routing_api_key_here
ROUTING_API_PROVIDER=openrouteservice # or 'mapbox'

# Geocoding API (Nominatim or Mapbox)
GEOCODING_API_KEY=your_geocoding_api_key_here
GEOCODING_API_PROVIDER=nominatim # or 'mapbox'

# Map Tiles Provider
MAP_PROVIDER=openstreetmap # or 'mapbox'
MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
